public class Exercise_5 {
    public static void main(String[] args) {
        int[] arr = new int[]{1, 2, 3, 4, 5, 5, 4, 3, 2, 1};
        boolean sym = true;
        for (int i = 0, j = arr.length - 1; j > i; i++, j--)
            if (arr[i] != arr[j]) {
                sym = false;
                System.out.println("\nНе симметричный ");
                break;
            }
        if (sym) System.out.println("\nСимметричный");
    }
}
